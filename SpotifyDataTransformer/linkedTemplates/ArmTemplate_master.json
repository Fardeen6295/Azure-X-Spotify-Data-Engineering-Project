{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SpotifyDataTransformer"
		},
		"SpotifyDataSource_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SpotifyDataSource'"
		},
		"datalakemedallion_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakemedallion'"
		},
		"SpotifyDataSource_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azurexspotifyproject.database.windows.net"
		},
		"SpotifyDataSource_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azureprojectdbkhan"
		},
		"SpotifyDataSource_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminkhan"
		},
		"datalakemedallion_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifyazureproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SpotifyDataTransformer_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SpotifyDataSource_password": {
						"value": "[parameters('SpotifyDataSource_password')]"
					},
					"datalakemedallion_accountKey": {
						"value": "[parameters('datalakemedallion_accountKey')]"
					},
					"SpotifyDataSource_properties_typeProperties_server": {
						"value": "[parameters('SpotifyDataSource_properties_typeProperties_server')]"
					},
					"SpotifyDataSource_properties_typeProperties_database": {
						"value": "[parameters('SpotifyDataSource_properties_typeProperties_database')]"
					},
					"SpotifyDataSource_properties_typeProperties_userName": {
						"value": "[parameters('SpotifyDataSource_properties_typeProperties_userName')]"
					},
					"datalakemedallion_properties_typeProperties_url": {
						"value": "[parameters('datalakemedallion_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}